{
  "name": "cms",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.52",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "eslint-config-prettier": "^6.10.1",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "^2.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^5.0.1",
    "react-typist": "^2.0.5",
    "typeface-roboto": "^0.0.75",
    "yarn": "^1.22.19"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:all": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "es:lint": "eslint --ext .js,.jsx .",
    "es:lint:fix": "eslint --fix --ext .js,.jsx .",
    "deploy": "rm -R build && rm -R .firebase && yarn build && firebase deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test:all --coverage",
      "pre-commit": "npm run es:lint:fix"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx}": [
      "npx prettier --write",
      "eslint src/*.js --fix-dry-run",
      "git add"
    ]
  }
}
